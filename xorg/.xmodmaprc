! Swap caps lock and escape
! remove Lock = Caps_Lock
! keysym Escape = Caps_Lock
! keysym Caps_Lock = Escape
! add Lock = Caps_Lock

! 105 = ctrl-r
! keycode 66 = Mode_switch
clear Mod4
clear Mod1
clear Control
add Mod4 = Control_R
add Control = Control_L

clear Mod1
clear Mod3
add Mod1 = Alt_L
add Mod3 = Alt_R
! keysym h = h H Left
! keysym l = l L Right
! keysym k = k K Up
! keysym j = j J Down

